<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="GameNetwork" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/GameNetwork" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
					<Add option="-D_DEBUG" />
					<Add option="-DPLATFORM=PLATFORM_WINDOWS" />
					<Add option="-DWINDOWS" />
				</Compiler>
				<Linker>
					<Add library="user32" />
					<Add library="ws2_32" />
				</Linker>
				<ExtraCommands>
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)..\Client\bin\$(TARGET_NAME)&quot;' />
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)..\Server\bin\$(TARGET_NAME)&quot;' />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/GameNetwork" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
					<Add option="-DPLATFORM=PLATFORM_WINDOWS" />
					<Add option="-DWINDOWS" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
					<Add library="ws2_32" />
				</Linker>
				<ExtraCommands>
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)..\Client\bin\$(TARGET_NAME)&quot;' />
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)..\Server\bin\$(TARGET_NAME)&quot;' />
				</ExtraCommands>
			</Target>
			<Target title="Linux">
				<Option output="GameNetwork" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Linux/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-fPIC" />
					<Add option="-DPLATFORM=PLATFORM_UNIX" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="cp libGameNetwork.so ../Client/libGameNetwork.so" />
					<Add after="cp libGameNetwork.so ../Server/libGameNetwork.so" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wnon-virtual-dtor" />
			<Add option="-Winit-self" />
			<Add option="-Wredundant-decls" />
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-DPLATFORM_WINDOWS=1" />
			<Add option="-DPLATFORM_MAC=2" />
			<Add option="-DPLATFORM_UNIX=3" />
			<Add directory="../" />
		</Compiler>
		<Unit filename="main.cpp" />
		<Unit filename="main.h" />
		<Unit filename="packet.cpp" />
		<Unit filename="packet.hpp" />
		<Unit filename="packetacklist.cpp" />
		<Unit filename="packetacklist.hpp" />
		<Unit filename="socket.cpp" />
		<Unit filename="socket.hpp" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
