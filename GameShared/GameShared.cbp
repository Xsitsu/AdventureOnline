<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="GameShared" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/GameShared" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
					<Add option="-D_DEBUG" />
					<Add option="-DPLATFORM=PLATFORM_WINDOWS" />
				</Compiler>
				<Linker>
					<Add library="user32" />
					<Add library="ws2_32" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/GameShared" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
					<Add option="-DPLATFORM=PLATFORM_WINDOWS" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
					<Add library="ws2_32" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DPLATFORM_WINDOWS=1" />
			<Add option="-DPLATFORM_MAC=2" />
			<Add option="-DPLATFORM_UNIX=3" />
		</Compiler>
		<ExtraCommands>
			<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)..\Client\bin\$(TARGET_NAME)&quot;' />
			<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)..\Server\bin\$(TARGET_NAME)&quot;' />
		</ExtraCommands>
		<Unit filename="main.cpp" />
		<Unit filename="main.h" />
		<Unit filename="packet.cpp" />
		<Unit filename="packet.hpp" />
		<Unit filename="packetacklist.cpp" />
		<Unit filename="packetacklist.hpp" />
		<Unit filename="socket.cpp" />
		<Unit filename="socket.hpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
